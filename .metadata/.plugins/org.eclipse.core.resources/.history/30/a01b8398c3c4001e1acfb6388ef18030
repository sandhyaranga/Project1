package forcondition1;
public class Multiplecondition {
	public static void main(String[] args) {
		int a=1;
		int b=1;
	for(int i=1;i<=10;i++) {
			if (i%2==0) {
				a=i*i;
				System.out.println("Square: "+a);
			}
			else {
				b=i*i*i;
				System.out.println("Cube: "+b) ;
			}}
	}
}
		
		
		
	
	
