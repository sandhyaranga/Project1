package day1;

public class UseMarker {
	public static void main(String[] args) {
		Marker m=new Marker();
		m.brand="Camlin";
		m.colour="Red";
		m.price=30;
		Marker m1=new Marker();
		m1.brand="Brile";
		m1.colour="Blue";
		m1.price=25;
		System.out.println(m.brand+" "+m.colour+" "+m.price);
		System.out.println(m1.brand+" "+m1.colour+" "+m1.price);	}

	

	
	
	
	